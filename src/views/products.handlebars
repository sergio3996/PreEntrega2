<h1>Productos</h1>
<article>
  <table>
    <tr>
      <th>Producto</th>
      <th>Categoria</th>
      <th>Precio</th>
      <th>Stock</th>
      <th></th>
    </tr>
    <tbody>
      {{#each payload}}
        <tr>
          <td>{{this.title}}</td>
          <td>{{this.category}}</td>
          <td>{{this.price}}</td>
          <td>{{this.stock}}</td>
          <td><button>Agregar al carrito</button></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
  <p>Pagina Actual: {{page}}</p>
  <a href="{{prevLink}}" role="button" {{#unless hasPrevPage}}disabled{{/unless}}>Anterior</a>
  <a href="{{nextLink}}" role="button" {{#unless hasNextPage}}disabled{{/unless}}>Siguiente</a>
</article>

<script src="/js/products.js"></script>